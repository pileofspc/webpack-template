{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "/*": ["*"],
            "@root/*": ["*"],
            "@modules/*": ["src/modules/*"],
            "@components/*": ["src/components/*"],
            "@pages/*": ["src/pages/*"],
            "@styles/*": ["src/assets/styles/*"],
            "@images/*": ["src/assets/images/*"],
            "@stores/*": ["src/stores/*"],
            "@types": ["src/types.ts"],
            "@composables": ["src/composables/*"]
        }
    }
}
